<div class="container">
  <h1>Timelines</h1>

  <% @timelines.each do |timeline| %>
    <div class="timeline">
      <img class="avatar" src="https://graph.facebook.com/<%= timeline.user.uid %>/picture?width=64&height=64" height="32px">
      
      <%= link_to timeline do %>
        <h4 class="title"><%= timeline.title %></h4>
      <% end %>
    </div>
  <% end %>

  <br>

  <%= link_to 'New Timeline', new_timeline_path, class: "btn btn-primary" %>
</div>
